{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri_nuxt",
  "version": "0.1.2",
  "identifier": "com.sksizer.example",
  "build": {
    "beforeDevCommand": "cd src-nuxt && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd src-nuxt && pnpm generate",
    "frontendDist": "../src-nuxt/.output/public"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tauri_nuxt",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
